<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Label?>
<?import javafx.scene.image.ImageView?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.layout.VBox?>

<VBox prefWidth="150" spacing="6" xmlns="http://javafx.com/javafx/17" xmlns:fx="http://javafx.com/fxml/1"
      style="-fx-border-color: #B0B0B0; -fx-border-width: 1; -fx-border-radius: 5; -fx-padding: 2;">
    <Label fx:id="tutorial" text="$tutorial" style="-fx-font-weight: bold; -fx-font-size: 16; -fx-text-fill: #262626"/>
    <HBox spacing="30">
        <Label text="Students enrolled" style="-fx-font-size: 14; -fx-text-fill: #262626"/>
        <Label fx:id="studentsEnrolled" text="\$students enrolled" style="-fx-font-size: 14; -fx-text-fill: #262626"/>
    </HBox>
</VBox>

